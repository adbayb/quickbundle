{
	"$schema": "https://turborepo.org/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"cache": true,
			"outputs": ["dist/**"]
		},
		"watch": {
			"dependsOn": ["^build"],
			"cache": false,
			"persistent": true
		},
		"start": {
			"dependsOn": ["build"],
			"cache": false,
			"persistent": true
		}
	}
}
